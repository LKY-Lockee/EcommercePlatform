{
	"name": "ecommerce-platform",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"preview-network": "vite preview --host",
		"build-only": "vite build",
		"type-check": "vue-tsc --build",
		"lint": "eslint . --fix",
		"format": "prettier --write src/",
		"server": "node dist/server/index.js",
		"server:dev": "tsx server/index.ts",
		"server:build": "tsc --project tsconfig.server.json",
		"dev:all": "concurrently \"npm run server:dev\" \"npm run dev\"",
		"build:all": "npm-run-all2 build server:build",
		"preview:all": "concurrently \"npm run server\" \"npm run preview-network\""
	},
	"dependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/multer": "^1.4.13",
		"axios": "^1.10.0",
		"bcryptjs": "^3.0.2",
		"concurrently": "^9.1.2",
		"cors": "^2.8.5",
		"dotenv": "^16.5.0",
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2",
		"multer": "^2.0.1",
		"mysql2": "^3.14.1",
		"nodemon": "^3.1.10",
		"pinia": "^3.0.3",
		"ts-node": "^10.9.2",
		"vite-plugin-vue-setup-extend": "^0.4.0",
		"vue": "^3.5.16",
		"vue-router": "^4.5.1",
		"vuestic-ui": "^1.10.3"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.2",
		"@types/node": "^24.0.1",
		"@vitejs/plugin-vue": "^5.2.4",
		"@vitejs/plugin-vue-jsx": "^4.2.0",
		"@vue/eslint-config-prettier": "^10.2.0",
		"@vue/eslint-config-typescript": "^14.5.0",
		"@vue/tsconfig": "^0.7.0",
		"eslint": "^9.29.0",
		"eslint-plugin-vue": "~10.2.0",
		"jiti": "^2.4.2",
		"npm-run-all2": "^8.0.4",
		"prettier": "3.5.3",
		"tsx": "^4.20.3",
		"typescript": "~5.8.3",
		"vite": "^6.3.5",
		"vite-plugin-vue-devtools": "^7.7.7",
		"vue-tsc": "^2.2.10"
	}
}
